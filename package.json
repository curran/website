{
  "name": "ejfox",
  "version": "1.0.0",
  "description": "EJ Fox.com",
  "author": "EJ Fox <ejfox@ejfox.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "serve": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "keybase": "nuxt generate;yes | cp -rf dist/* /keybase/public/ejfox/;echo 'Copied to Keybase public folder'",
    "publish": "nuxt generate;npm run keybase;git stage -A;git commit -m 'publish';git push;echo 'Published on Keybase and GitHub (Netlify)'",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/feed": "^1.1.0",
    "@nuxtjs/google-analytics": "^2.2.0",
    "cheerio": "^1.0.0-rc.2",
    "fs": "0.0.1-security",
    "howler": "^2.1.1",
    "libxmljs": "^0.19.5",
    "lodash": "^4.17.11",
    "marked": "^0.6.1",
    "nuxt": "^1.4.5",
    "stylus": "^0.54.5",
    "truncate": "^2.0.1",
    "uri-js": "^4.2.2",
    "url-parse": ">=1.4.3",
    "vue-howler": "^0.7.0",
    "vue-moment": "^3.2.0",
    "vue-style-loader": "^4.1.2",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
